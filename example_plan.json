{
  "rules": [
    {
      "name": "delay_ping_1000",
      "delay": 1000,
      "percentage": 25,
      "command": "ping"
    },
    {
      "name": "delay_any_containing_foo_or_bar_1000",
      "delay": 1000,
      "rawMatch": ["foo", "bar"]
    },
    {
      "name": "delay_set_my-key_1000",
      "delay": 1000,
      "command": "set my-key"
    }
  ]
}
